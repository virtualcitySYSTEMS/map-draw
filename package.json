{
  "name": "@vcmap/draw",
  "version": "1.0.0",
  "description": "VC Map plugin for drawing and editing 2D an 3D geometries in the map.",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "prepublishOnly": "vcmplugin build",
    "build": "vcmplugin build",
    "pack": "vcmplugin pack",
    "start": "vcmplugin serve",
    "preview": "vcmplugin preview",
    "buildStagingApp": "vcmplugin buildStagingApp",
    "lint:js": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "format": "prettier --write --list-different . && npm run lint:js -- --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "author": "Virtual City Systems",
  "license": "MIT",
  "keywords": [
    "vcmap",
    "plugin"
  ],
  "files": [
    "src/",
    "dist/",
    "plugin-assets/",
    "LICENSE.md",
    "README.md"
  ],
  "exports": {
    ".": "./src/index.js",
    "./dist": "./dist/index.js"
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "@vcsuite/eslint-config/vue"
  },
  "prettier": "@vcsuite/eslint-config/prettier.js",
  "peerDependencies": {
    "@vcmap-cesium/engine": "~2.3.1",
    "@vcmap/core": "^5.0.0-rc.31",
    "@vcmap/ui": "^5.0.0-rc.24",
    "ol": "~7.3.0",
    "vue": "~2.7.3",
    "vuetify": "~2.6.7"
  },
  "devDependencies": {
    "@vcmap/plugin-cli": "^2.1.8",
    "@vcsuite/eslint-config": "^3.0.3",
    "@vitest/coverage-c8": "^0.29.2",
    "jest-canvas-mock": "^2.4.0",
    "jsdom": "^21.1.0",
    "vitest": "^0.29.2"
  }
}
